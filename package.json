{
  "name": "magellania-travel-system",
  "version": "1.0.0",
  "description": "Система управления сметами и CRM для турагентства",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "node start-simple.cjs",
    "api": "cd api-server && node working-server.js",
    "status": "node check-status.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "chart.js": "^4.4.2",
    "date-fns": "^3.6.0",
    "dexie": "^4.0.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lucide-vue-next": "^0.365.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.0",
    "vue-draggable-plus": "^0.3.5",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^8.1.0",
    "@types/papaparse": "^5.3.14",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue",
    "travel",
    "crm",
    "erp",
    "estimates",
    "tours"
  ],
  "author": "Magellania Travel",
  "license": "MIT"
}
