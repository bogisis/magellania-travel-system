<!-- .env.example -->
# Настройки приложения
VITE_APP_TITLE=MAGELLANIA Travel System
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Система управления сметами и CRM для турагентства

# API конфигурация (для будущего использования)
VITE_API_BASE_URL=https://api.example.com
VITE_API_VERSION=v1

# Внешние сервисы
VITE_CURRENCY_API_KEY=your_currency_api_key_here
VITE_MAPS_API_KEY=your_maps_api_key_here

# Настройки для продакшена
VITE_ENABLE_ANALYTICS=false
VITE_SENTRY_DSN=your_sentry_dsn_here

# Режим отладки
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info